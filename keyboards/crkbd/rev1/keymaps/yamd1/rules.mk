RGBLIGHT_ENABLE = yes
OLED_ENABLE = yes
COMBO_ENABLE=yes
HOME_ROW_COMBO_EBABLE=yes
CAPS_WORD_ENABLE = yes

# コンパイル対象を指定する。コメントアウトするとavr用にコンパイルされる
# CONVERT_TO=elite_pi

# key_overridesが未定義の場合によって引き起こされるコンパイルエラーへ対応
# SEE: https://chatgpt.com/c/67f5b8b9-07e8-8009-b9d8-4e1dd24f6ec1
KEY_OVERRIDE_ENABLE = yes

# Keymap Introspection を無効化
KEYMAP_INTROSPECTION_ENABLE = no
